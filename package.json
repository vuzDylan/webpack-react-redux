{
  "name": "Webpack-react-redux",
  "version": "4.0.0",
  "description": "Boilerplate for setting up hot reloading react project",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuzDylan/webpack-react-redux.git"
  },
  "author": "Dylan Vuz",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "NODE_ENV=production BABEL_ENV=production ./node_modules/.bin/webpack --config webpack.config.production.js",
    "postbuild": "cp app/index.prod.html dist/index.html",
    "preprod": "npm run build",
    "prod": "NODE_ENV=production BABEL_ENV=production node server.prod.js",
    "lint": "eslint app",
    "validate": "npm ls"
  },
  "pre-commit": [
    "lint",
    "validate"
  ],
  "dependencies": {
    "babel-runtime": "^6.3.19",
    "bootstrap": "^3.3.7",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.3.21",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.8.0",
    "precommit-hook": "^3.0.0",
    "react-transform-hmr": "^1.0.1",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
